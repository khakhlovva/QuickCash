{
  "name": "@rnr/starter-base",
  "main": "index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start -c --ios",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "deps": "pnpm dlx npm-check-updates",
    "deps:update": "pnpm deps -u",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./styles/global.css -o ./node_modules/.cache/nativewind/global.css",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "format:check": "prettier \"**/*.{js,mjs,ts,tsx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "format:write": "pnpm format:check --write",
    "format": "pnpm lint:fix && pnpm format:write"
  },
  "packageManager": "pnpm@9.14.2",
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.5.0",
    "@prisma/react-native": "^6.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.0.14",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^53.0.9",
    "expo-av": "^15.0.2",
    "expo-document-picker": "^13.0.3",
    "expo-linking": "~7.1.4",
    "expo-navigation-bar": "~4.2.4",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-video": "^2.0.6",
    "lucide-react-native": "^0.460.0",
    "nativewind": "^4.1.23",
    "prisma": "^6.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-native": "0.79.2",
    "react-native-popup-menu": "^0.16.1",
    "react-native-quick-base64": "^2.1.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.9.2",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.2.1",
    "react-native-web": "^0.20.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "~18.3.19",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.2"
  }
}
